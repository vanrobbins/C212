<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="200.0" maxWidth="300.0" prefHeight="175.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vanar.lab8gui.GUIController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" layoutX="20.0" layoutY="80.0" prefWidth="300.0" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="run" mnemonicParsing="false"  onAction="#saveRunAction" text="Run + Save">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="createGraph" mnemonicParsing="false" text="Generate Graph" onAction="#saveGraph" disable="true">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" prefWidth="300.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0">
         <children>
            <Label text="Comparing Insertion, Selection, and Merge Sorting Algorithms">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </FlowPane>
   </children>
</AnchorPane>
